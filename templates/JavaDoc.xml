<templateSet group="JavaDoc">
  <template name="@link" value="{@link $LINK$}" description="Javadoc @link" toReformat="false" toShortenFQNames="true">
    <variable name="LINK" expression="complete()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_COMMENT" value="true" />
    </context>
  </template>
</templateSet>